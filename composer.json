{
    "name": "phlib/beanstalk",
    "description": "Library for handling beanstalk connections.",
    "type": "library",
    "keywords": ["beanstalk", "pool", "job", "queue"],
    "license": "MIT",
    "homepage": "https://github.com/phlib/beanstalk",
    "authors": [
        {
            "name": "James Dempster",
            "email": "james.dempster@emailcenteruk.com",
            "role": "developer"
        },
        {
            "name": "Luke Richards",
            "email": "luke.richards@emailcenteruk.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/phlib/console"
        }
    ],
    "require": {
        "php": "~5.4",
        "symfony/console": "^2.7",
        "phlib/console": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "php-mock/php-mock-phpunit": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\Beanstalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\Beanstalk\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/beanstalk"]
}
